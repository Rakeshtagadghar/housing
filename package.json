{
  "name": "housinganywhere",
  "version": "1.0.0",
  "description": "",
  "keywords": [],
  "dependencies": {
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-regular-svg-icons": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@mui/material": "^5.8.7",
    "@types/styled-components": "^5.1.25",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-router-dom": "^6.3.0",
    "react-scripts": "^5.0.0",
    "rimraf": "^3.0.2",
    "styled-components": "^5.3.5",
    "typescript": "^4.7.4",
    "vitest": "^0.18.0"
  },
  "devDependencies": {
    "@stylelint/postcss-css-in-js": "^0.38.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.5",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "bootstrap": "^5.1.3",
    "eslint": "^8.19.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss-scss": "^4.0.4",
    "prettier": "^2.7.1",
    "sass": "^1.45.1",
    "stylelint": "^14.9.1",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-standard-scss": "^4.0.0",
    "vite": "^2.9.14",
    "vite-plugin-svgr": "^2.2.0"
  },
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "clean": "rimraf ./node_modules",
    "test": "react-scripts test --env=jsdom",
    "test-cov": "react-scripts test --env=jsdom --coverage",
    "lint-check": "eslint src --max-warnings 0",
    "lint-fix": "eslint src --max-warnings 0 --fix",
    "prettier-check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,md,mdx,css,scss}\" --loglevel=error",
    "prettier-fix": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,md,mdx,css,scss}\" --loglevel=error && git add .",
    "stylelint-check": "stylelint src/**/*.{css,scss,js,ts,tsx} --max-warnings 0",
    "stylelint-fix": "stylelint src/**/*.{css,scss,js} --max-warnings 0  --fix"
  },
  "resolutions": {
    "styled-components": "^5"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "coverageReporters": [
      "text"
    ]
  }
}
