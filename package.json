{
  "name": "housinganywhere",
  "version": "1.0.0",
  "description": "",
  "keywords": [],
  "dependencies": {
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@fortawesome/fontawesome-free": "^6.1.1",
    "@mui/material": "^5.8.7",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-router-dom": "^6.3.0",
    "rimraf": "^3.0.2",
    "styled-components": "^5.3.5",
    "typescript": "^4.7.4",
    "vitest": "^0.18.0",
    "react-scripts": "^5.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.5",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "bootstrap": "^5.1.3",
    "sass": "^1.45.1",
    "vite": "^2.9.14",
    "vite-plugin-svgr": "^2.2.0"
  },
  "scripts": {
    "start": "vite",
    "build": " vite build",
    "serve": "vite preview",
    "clean": "rimraf ./node_modules",
    "test": "react-scripts test --env=jsdom",
    "test-cov": "react-scripts test --env=jsdom --coverage"
  },
  "resolutions": {
    "styled-components": "^5"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "coverageReporters": [
      "text"
    ]
  }
}
